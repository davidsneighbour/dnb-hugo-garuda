{
  "name": "dnb-hugo-garuda",
  "version": "2020.0.0",
  "author": {
    "name": "Patrick Kollitsch",
    "email": "patrick@davids-neighbour.com",
    "web": "https://davids-neighbour.com"
  },
  "contributors": [
    {
      "name": "Patrick Kollitsch",
      "email": "patrick@davids-neighbour.com",
      "web": "https://davids-neighbour.com"
    }
  ],
  "description": "An opiniated theme for Hugo by David's Neighbour",
  "main": "webpack.config.js",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/davidsneighbour/samui-samui.de.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "scripts": {
    "algolia": "hugo --quiet && atomic-algolia && rm -rf public",
    "lighthouse": "lighthouse https://samui-samui.de --budget-path=budget.json --view",
    "css": "node-sass -q --include-path public/javascripts/node_modules --output-style compressed --precision 6 assets/scss/default.scss -o public/theme/assets/css/ ",
    "css2": "node-sass -q --include-path public/javascripts/node_modules --output-style compressed --precision 6 assets/scss/ -o public/theme/assets/css/ ",
    "build-changelog": "./bin/create-changelog.sh > CHANGELOG.md",
    "postcss": "postcss public/theme/assets/css/default.css -r",
    "postcss2": "postcss public/theme/assets/css/*.css -r",
    "nightwatch": "nightwatch -c tests/nightwatch.conf.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "preversion": "if [ \"$(git rev-parse --abbrev-ref HEAD)\" = main ] ; then echo Preversion checks passed. ; else echo Error: Not on branch release. ; exit 1 ; fi",
    "release": "standard-version --release-as patch -a -t \"\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"\" && ./bin/release-hook-postrelease.sh",
    "test": "nightwatch",
    "all-tests": "./vendor/bin/codecept run && npm run nightwatch"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "atomic-algolia": "0.3.19",
    "autoprefixer": "10.0.0",
    "bootstrap": "4.5.2",
    "cookieconsent": "3.1.1",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "minimist": "1.2.5",
    "moment": "2.29.0",
    "popper.js": "1.16.1",
    "postcss-cli": "8.0.0",
    "standard-version": "9.0.0",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@popperjs/core": "2.5.3",
    "@sentry/cli": "1.58.0",
    "babel-loader": "8.1.0",
    "chromedriver": "85.0.1",
    "clean-css-cli": "4.3.0",
    "concat-cli": "4.0.0",
    "critical": "2.0.4",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "csscomb": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-vue": "6.2.2",
    "file-loader": "6.1.0",
    "geckodriver": "1.20.0",
    "git": "0.1.5",
    "http-vue-loader": "1.4.2",
    "imagemin-cli": "6.0.0",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-pngquant": "9.0.1",
    "instantsearch.js": "4.8.3",
    "jquery": "3.5.1",
    "lodash": "4.17.20",
    "lodash.merge": "4.6.2",
    "lodash.template": "4.5.0",
    "mem": "6.1.1",
    "nightwatch": "1.4.3",
    "node-sass": "4.14.1",
    "normalize-css": "2.3.1",
    "pixrem": "5.0.0",
    "precss": "4.0.0",
    "prettier": "2.1.2",
    "rework-npm-cli": "0.1.3",
    "sass": "1.26.11",
    "sass-loader": "10.0.2",
    "selenium-server": "3.141.59",
    "stylelint": "13.7.2",
    "stylelint-config-standard": "20.0.0",
    "textlint": "11.7.6",
    "textlint-plugin-html": "0.2.0",
    "textlint-rule-abbr-within-parentheses": "1.0.2",
    "textlint-rule-alex": "3.0.0",
    "textlint-rule-apostrophe": "2.0.0",
    "textlint-rule-common-misspellings": "1.0.1",
    "textlint-rule-date-weekday-mismatch": "1.0.5",
    "textlint-rule-en-capitalization": "2.0.2",
    "textlint-rule-en-spell": "1.1.1",
    "textlint-rule-footnote-order": "1.0.3",
    "textlint-rule-max-comma": "1.0.4",
    "textlint-rule-no-dead-link": "4.7.0",
    "textlint-rule-no-empty-section": "1.1.0",
    "textlint-rule-no-exclamation-question-mark": "1.1.0",
    "textlint-rule-no-start-duplicated-conjunction": "2.0.2",
    "textlint-rule-no-todo": "2.0.1",
    "textlint-rule-rousseau": "1.4.6",
    "textlint-rule-spelling": "0.2.0",
    "textlint-rule-stop-words": "2.0.7",
    "textlint-rule-terminology": "2.1.4",
    "textlint-rule-unexpanded-acronym": "1.2.4",
    "uglify-js": "3.11.0",
    "uncss": "0.17.3",
    "vue": "^3",
    "vue-loader": "15.9.3",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "@textlint-rule/textlint-rule-no-invalid-control-character": "1.2.0",
    "@textlint-rule/textlint-rule-no-unmatched-pair": "1.0.7",
    "axios": "0.20.0",
    "bootstrap-vue": "2.17.3",
    "cz-customizable-ghooks": "2.0.0",
    "postcss": "8.0.9",
    "vue-router": "3.4.5",
    "vuex": "3.5.1"
  },
  "standard-version": {
    "scripts": {
      "prerelease": "./bin/release-hook-prerelease.sh",
      "postbump": "./bin/release-hook-postbump.sh",
      "postcommit": "./bin/release-hook-postcommit.sh",
      "posttag": "./bin/release-hook-posttag.sh"
    },
    "skip": {
      "changelog": true
    },
    "bumpFiles": [
      {
        "filename": ".version",
        "type": "plain-text"
      },
      {
        "filename": "package.json",
        "type": "json"
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "indentation": 2,
      "string-quotes": "single",
      "no-duplicate-selectors": true,
      "color-hex-case": "lower",
      "color-hex-length": "long",
      "color-named": "never",
      "selector-attribute-quotes": "always",
      "declaration-block-trailing-semicolon": "always",
      "declaration-colon-space-before": "never",
      "declaration-colon-space-after": "always",
      "number-leading-zero": "always",
      "function-url-quotes": "always",
      "font-weight-notation": "numeric",
      "font-family-name-quotes": "always-where-recommended",
      "comment-empty-line-before": "always"
    },
    "ignoreFiles": [
      "public/theme/assets/**/*.css"
    ]
  },
  "postcss": {
    "plugins": {
      "precss": {},
      "autoprefixer": {
        "flexbox": true
      },
      "pixrem": {},
      "cssnano": {
        "preset": "default"
      },
      "stylelint": {}
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es2015": true
    },
    "extends": [
      "airbnb"
    ],
    "globals": {
      "axios": "writable",
      "Vue": "writable"
    },
    "ignorePatterns": [
      "temp.js",
      "**/vendor/*.js"
    ]
  },
  "textlint": {
    "rules": {
      "no-todo": true,
      "textlint-rule-no-start-duplicated-conjunction": true,
      "max-comma": {
        "max": 3
      },
      "no-exclamation-question-mark": true,
      "no-dead-link": {
        "checkRelative": true,
        "baseURI": null,
        "ignore": [],
        "preferGET": [],
        "ignoreRedirects": false,
        "retry": 3,
        "userAgent": "textlint-rule-no-dead-link/1.0"
      },
      "en-spell": true,
      "en-capitalization": {
        "allowHeading": true,
        "allowFigures": true,
        "allowLists": true,
        "allowWords": []
      },
      "stop-words": {
        "defaultWords": true,
        "skip": [
          "Blockquote"
        ],
        "exclude": [
          "utilize",
          "period of time"
        ],
        "words": "config/stopwords.txt"
      },
      "apostrophe": true,
      "common-misspellings": {
        "ignore": []
      },
      "date-weekday-mismatch": true,
      "terminology": {
        "defaultTerms": true,
        "skip": [
          "Blockquote"
        ],
        "terms": "config/terms.json",
        "exclude": [
          "CSS"
        ]
      },
      "alex": {
        "allow": [
          "boogeyman-boogeywoman"
        ]
      },
      "rousseau": {
        "showLevels": [
          "suggestion",
          "warning",
          "error"
        ]
      },
      "abbr-within-parentheses": true,
      "spelling": {
        "language": "en",
        "skipPatterns": [
          "JavaScript"
        ],
        "wordDefinitionRegexp": "/[\\w']+/g",
        "suggestCorrections": true
      },
      "unexpanded-acronym": {
        "min_acronym_len": 3
      },
      "footnote-order": true,
      "@textlint-rule/no-unmatched-pair": true,
      "@textlint-rule/no-invalid-control-character": true,
      "no-empty-section": true,
      "no-start-duplicated-conjunction": {
        "interval": 2
      }
    },
    "plugins": [
      "html"
    ]
  }
}
